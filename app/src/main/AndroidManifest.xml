<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.lifeistech.com.memo">

    <application
        android:name=".MemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="プロジェクト"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateActivity" />
        <activity
            android:name=".FeedbackList"
            android:label="フィードバック一覧" />
        <activity
            android:name=".CreateFeedback"
            android:label="本日のフィードバックを記入" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".FeedbackdetailActivity"></activity>
    </application>

</manifest>